{
  "name": "demo-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc --project tsconfig.compile.json",
    "start": "tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "test:lint": "eslint '**/*.{ts,js}'",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --dataSource src/datasources/postgres.datasource.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate --dataSource src/datasources/postgres.datasource.ts",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@tsed/ajv": "^7.18.4",
    "@tsed/common": "^7.18.4",
    "@tsed/core": "^7.18.4",
    "@tsed/di": "^7.18.4",
    "@tsed/engines": "^7.18.4",
    "@tsed/exceptions": "^7.18.4",
    "@tsed/json-mapper": "^7.18.4",
    "@tsed/logger": "^6.3.4",
    "@tsed/logger-file": "^6.3.4",
    "@tsed/openspec": "^7.18.4",
    "@tsed/platform-cache": "^7.18.4",
    "@tsed/platform-exceptions": "^7.18.4",
    "@tsed/platform-express": "^7.18.4",
    "@tsed/platform-log-middleware": "^7.18.4",
    "@tsed/platform-middlewares": "^7.18.4",
    "@tsed/platform-params": "^7.18.4",
    "@tsed/platform-response-filter": "^7.18.4",
    "@tsed/platform-views": "^7.18.4",
    "@tsed/schema": "^7.18.4",
    "@tsed/swagger": "^7.18.4",
    "ajv": "^8.12.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "pg": "^8.10.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.4.4",
    "@tsed/cli-plugin-typeorm": "4.4.4",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  },
  "tsed": {
